<div class="bg-gray-50">
  <div class="max-w-screen-xl mx-auto py-12 px-4 sm:px-6 lg:py-16 lg:px-8 lg:flex lg:items-center lg:justify-between">
    <h2 class="text-3xl leading-9 font-extrabold tracking-tight text-gray-900 sm:text-4xl sm:leading-10">
      <% if @current_structure.logo.attached? %>
        <%= image_tag url_for(@current_structure.logo), class: "inline-block h-12 w-12" %> Gestion de la structure
      <% else %>
        <%= image_tag 'logo.png', class: "inline-block h-12 w-12" %> Gestion de la structure
      <% end %>
      <br/>
      <span class="text-indigo-600"><%= @current_structure.name %>.</span>
    </h2>
    <div class="mt-8 flex lg:flex-shrink-0 lg:mt-0">
      <div class="inline-flex rounded-md shadow">
        <%= link_to "Update structure", setup_structure_index_path(structure_id: params[:structure_id]), class: "inline-flex items-center justify-center px-5 py-3 border border-transparent text-base leading-6 font-medium rounded-md text-white bg-indigo-600 hover:bg-indigo-500 focus:outline-none focus:shadow-outline transition duration-150 ease-in-out" %>
      </div>
    </div>
  </div>
</div>


